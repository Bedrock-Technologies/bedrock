{
  "name": "cra-template",
  "version": "1.0.0",
  "proxy": "http://localhost:3001",
  "scripts": {
    "build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --colors",
    "coverage": "react-app-rewired test --coverage",
    "precommit": "lint-staged",
    "api": "node src/api/index.ts"
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "cross-env CI=true react-scripts test --colors --findRelatedTests"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!**/*.d.ts?(x)",
      "!**/*.stories.ts?(x)",
      "!**/__*__/**"
    ]
  },
  "browserslist": {
    "production": [
      "chrome > 60",
      "firefox > 60",
      "safari > 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/forms": "^0.4.0",
    "@terra-money/terra.js": "^3.0.2",
    "@terra-money/wallet-provider": "^3.6.5",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "ipfs-core": "^0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.3",
    "tailwindcss": "^3.0.17"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "lint-staged": "^11.2.6",
    "process": "^0.11.10",
    "react-app-rewired": "^2.1.11",
    "react-scripts": "5.0.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.4.4"
  }
}
